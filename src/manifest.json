{
  "manifest_version": 2,

  "name": "DownloadMaster",
  "description": "Automatically sorts your downloaded files into your Dropbox account",
  "version": "1.0",
  "background": {"scripts": ["js/jquery-1.10.2.min.js"]},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
